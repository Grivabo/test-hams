{
    "type" : "project",
    "license" : "proprietary",
    "require" : {
        "php" : "^8.2",
        "ext-ctype" : "*",
        "ext-iconv" : "*",
        "symfony/property-access" : "^6.3",
        "symfony/validator" : "^6.3"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^10.2",
        "symfony/config" : "^6.3",
        "symfony/dependency-injection" : "^6.3",
        "symfony/finder" : "^6.3"
    },
    "config" : {
        "preferred-install" : {
            "*" : "dist"
        },
        "sort-packages" : true
    },
    "autoload" : {
        "psr-4" : {
            "App\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "App\\Tests\\" : "tests/"
        }
    }
}
